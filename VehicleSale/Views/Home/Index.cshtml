<style>
    .feature_background {
        background: #00ff00;
        color: #333;
        border: #d95b5b 1px solid;
    }
</style>


<script>

    var person_count = 0;
    var feature_array = [];
    var vehicle_type;
    function removeA(arr) {
        var what, a = arguments, L = a.length, ax;
        while (L > 1 && arr.length) {
            what = a[--L];
            while ((ax = arr.indexOf(what)) !== -1) {
                arr.splice(ax, 1);
            }
        }
        return arr;
    }

    function vehicle_types(ctrl) {
        $('#c_scroll li').attr("class", "");

        vehicle_type = $(ctrl).data('html');
        vehicle_type = $(ctrl).attr('data-bind');
        if ($(ctrl).parent("li").attr("class") == "") {
            $(ctrl).parent("li").attr("class", "feature_background");
        }
        else {
            $(ctrl).parent("li").attr("class", "");
        }
    }
    function personcount(ctrl) {
        var id = $(ctrl).attr('id');
        debugger;
        person_count = id;
        for (var i = 0; i < parseInt(id) ; i++) {
            $('#p_setid img').eq(i).attr("class", "p_image");
            $('img.p_image').attr("src", "/ProductImage/im.png");
        }
        for (var i = parseInt(id) ; i <= 10 ; i++) {
            $('#p_setid img').eq(i).attr("class", "no_image");
            $('img.no_image').attr("src", "/ProductImage/brightimage.png");
        }
        var new_value = parseInt(id) * 10;
        $("#p_scroll").slider('values', [new_value, 110]).change();
    }
    function SelectFeature(ctrl) {

        //   var id=$(ctrl).find('span').attr("id");

        if ($(ctrl).parent("li").attr("class") == "") {
            $(ctrl).parent("li").attr("class", "feature_background");
            // $(ctrl).find('span').html("<b style='color:black;'>SELECTED</b>");
            feature_array.push($(ctrl).data('html'));
        }
        else {
            $(ctrl).parent("li").attr("class", "");
            //  $(ctrl).find('span').html("");

            removeA(feature_array, $(ctrl).data('html'));
        }
    }
    var count_ui = 1;
    (function () {
        $("#p_scroll").slider({
            range: true,
            min: 1,
            max: 110,
            values: [1, 110],
            step: 10,
            slide: function (event, ui) {
                $("img.p_image").attr("class", "no_image");
                console.log("val" + ui.values[0]);
                var sum = parseInt(ui.values[0]) - parseInt(count_ui);
                console.log("sum" + sum);
                var div = sum / 10;
                console.log("div" + div);
                //  var sum = parseInt(ui.values[1]) - parseInt(ui.values[0]);
                //  var div = sum / 10;
                for (var i = 0; i < div; i++) {
                    $('#p_setid img').eq(i).attr("class", "p_image");
                }
                //$('#p_setid li').attr("class", "p_image");
                $('img.p_image').attr("src", "/ProductImage/im.png");
                $('img.no_image').attr("src", "/ProductImage/brightimage.png");
                person_count = div;
                handlevalues = $("#p_scroll").slider('values');
            },
        });

    })();


    function Back_Next(ctrl) {

        console.log($(ctrl).attr("href"));

        if ($(ctrl).attr("href") == "#feature" && parseInt(person_count) > 0) {
            $('#feature').show();
            $('#passengers').hide();
            //  $('#type').hide();
        }
        else if ($(ctrl).attr("href") == "#passengers") {
            $('#feature').hide();
            $('#passengers').show();
            // $('#type').hide();
        }
        //else if ($(ctrl).attr("href") == "#type" && feature_array.length > 0) {
        //    $('#feature').hide();
        //    $('#passengers').hide();
        //    $('#type').show();
        //}

    }
    function Result(e) {
        
        $.ajax({
            type: "POST",
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            url: "/Home/GoToList",
            data: "{'person':'" + person_count + "', 'feature':'" + feature_array + "'}",
            success: function (Record) {
                window.location.href = "/Home/ListingPage";

            }
        });
    }

</script>

