<script>
    var isblank = /^(\s)+$/;
    var isValidEmail = /^\w+((-\w+)|(\.\w+))*\@@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/;


    $('#btn_Message').click(function () {
        var Name = $('#msg_Name').val();
        var Email = $('#msg_Email').val();
        var Phone = $('#msg_Phone').val();
        var Message = $('#msg_Message').val();
        if (Name == '') {
            $("#err_Message").html('Please enter Name.');
            return false;
        }
        else if (Email == '') {
            $("#err_Message").html('Please enter Email.');
        }
        else if (Email.search(isValidEmail) == -1) {
            $("#err_Message").html('Please enter Correct Email.');
            return false;
        }
        else if (Phone == '') {
            $("#err_Message").html('Please enter Phone.');
            return false;
        }
        else if (Message == '') {
            $("#err_Message").html('Please enter Message');
            return false;
        }
        else {
            $.ajax({
                url: "/Home/MessageSenderDetails",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                data: "{'Name':'" + Name + "', 'Email':'" + Email + "', 'Phone':'" + Phone + "', 'Message':'" + Message + "'}",
                type: "post", success: function (data) {

                    $("#err_Message").html('Message sent sucessfully.');
                    $('#msg_Name').val('');
                    $('#msg_Email').val('');
                    $('#msg_Phone').val('');
                    $('#msg_Message').val('');

                }, error: function (data) {
                    return false;
                }
            });

        }
    });
    $('#btn_Subscribe').click(function () {

        var Email = $("#txt_Email").val();

        if (Email == '') {
            $("#error").html('Please enter Email.');
            return false;
        }
        else if (Email.search(isValidEmail) == -1) {
            $("#error").html('Please enter Correct Email.');
            return false;
        }
        else {
            $.ajax({
                url: "/Home/NewsletterSubscribe?Email=" + Email,
                type: "post", success: function (data) {
                    if (data == 'true') {
                        $("#error").html('Sucessfully Subscribed.');

                        $("#txt_Email").val("");
                    }
                    if (data == 'Already') {
                        $("#error").html('You are already Subscribed.');

                        $("#txt_Email").val("");

                    }
                    return true;
                }, error: function (data) {
                    return false;
                }
            });
        }
    });
